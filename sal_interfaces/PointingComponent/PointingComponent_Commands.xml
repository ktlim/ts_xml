            <IDL_Type>long</IDL_Type>
            <Enumeration>current,next,prospective</Enumeration>
            <Units/>
            <Count>1</Count>
        </item>
        <item>
            <EFDB_Name>frame</EFDB_Name>
            <Description>Target co-ordinate reference frame</Description>
            <IDL_Type>long</IDL_Type>
            <Enumeration>fk5,icrs</Enumeration>
            <Units/>
            <Count>1</Count>
        </item>
