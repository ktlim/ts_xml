    <EFDB_Topic>FiberSpectrograph_logevent_rejectedCommand</EFDB_Topic>
    <Alias>rejectedCommand</Alias>
    <Description>Command is not accepted at the current state, or parameter specified in the command are invalid or out of range</Description>
    <item>
        <EFDB_Name>commandValue</EFDB_Name>
        <Description>command that was rejected</Description>
        <IDL_Type>unsigned short</IDL_Type>
        <Units>unitless</Units>
        <Count>1</Count>
    </item>
    <item>
        <EFDB_Name>detailedState</EFDB_Name>
        <Description>The current state that the control system is in</Description>
        <IDL_Type>unsigned short</IDL_Type>
        <Units>unitless</Units>
        <Count>1</Count>
    </item>
    <item>
        <EFDB_Name>timestamp</EFDB_Name>
        <Description>Timestamp when the command was rejected</Description>
        <IDL_Type>double</IDL_Type>
        <Units>unitless</Units>
        <Count>1</Count>
    </item>
</SALEvent>
